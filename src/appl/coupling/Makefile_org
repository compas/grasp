Coupling_structures.o : Coupling_structures.f90 Coupling_constants.o
	$(FC) $(FFL) Coupling_structures.f90

Coupling_constants.o : Coupling_constants.f90
	$(FC) $(FFL) Coupling_constants.f90

sqlsf.o : sqlsf.f
	$(FC) $(FFL) sqlsf.f

clean:
	-rm -f *.o core *.mod
